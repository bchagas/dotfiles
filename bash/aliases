alias ..='cd ..'
alias gl="git log --oneline --graph --color --pretty=format:'(%an) - %s - %h '"
alias grm="git status | grep deleted | awk '{print \$3}' | xargs git rm"

#misc
alias gitx="/Applications/GitX.app/Contents/MacOS/GitX &"
alias psgrep="ps aux | egrep -v egrep | egrep"
alias lock="/System/Library/CoreServices/Menu\ Extras/user.menu/Contents/Resources/CGSession -suspend"
alias top="top -o rsize"
alias make="make -j 2"
alias cleanup="sudo rm -rf /private/var/log/asl/*"
alias workspace="cd /Users/brunochagas/Documents/Workspace/"
alias pstart="pg_ctl -D /usr/local/var/postgres -l /usr/local/var/postgres/server.log start"
alias pstop="pg_ctl -D /usr/local/var/postgres stop -s -m fast"
alias gems="cd /Users/brunochagas/Documents/workspace/gems"
alias be="bundle exec"
alias iPhoneSimulator="open /Applications/Xcode.app/Contents/Developer/Platforms/iPhoneSimulator.platform/Developer/Applications/iPhone\ Simulator.app"
alias dnsFlush="sudo killall -HUP mDNSResponder"
alias hubbiz_staging="ssh hubbiz@lauper.connecorp.com && cd /home/hubbiz/app/current"
alias vundle='vim +BundleInstall +qall'
alias sitespeed='sudo docker run --privileged --rm -v "$(pwd)":/sitespeed.io sitespeedio/sitespeed.io sitespeed.io -u -b firefox'
